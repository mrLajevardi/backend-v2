  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max-http-header-size=80000 dist/main",
    "pm2:reload": "pm2 reload arad-v2",
    "pm2:stop": "pm2 stop arad-v2",
    "lint:devq": "eslint  --quiet  \"{src,apps,libs,test}/**/*.ts\"  --fix --rulesdir eslint-rules/",
    "lint:dev": "eslint   \"{src,apps,libs,test}/**/*.ts\"  --fix --rulesdir eslint-rules/",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" -f ./node_modules/eslint-junit/index.js --rulesdir eslint-rules/",
    "test:dev": "cross-env NODE_ENV=test jest -max-workers=50%",
    "test:leak": "cross-env NODE_ENV=test node   ./node_modules/.bin/jest    --detectLeaks --runInBand --logHeapUsage",
    "test:leak2": "cross-env NODE_ENV=test node  ./node_modules/.bin/jest --runInBand --logHeapUsage ",
    "test:clearCache": "NODE_ENV=test jest --clear-cache",
    "test": "cross-env NODE_ENV=test jest --max-workers=10 --ci --reporters=default --reporters=jest-junit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node  --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d ./src/infrastructure/database/datasource.ts",
    "migration:generate": "npm run typeorm -- migration:generate ./src/infrastructure/database/migrations/%npm_config_name% -d ./src/infrastructure/database/datasource.ts",
    "migration:create": "npm run typeorm -- migration:create ./src/infrastructure/database/migrations/%npm_config_name%",
    "migration:revert": "npm run typeorm -- -d ./src/infrastructure/database/datasource.ts migration:revert"
  },
